\DeclareDatamodelFields[type=field, datatype=literal]{
  acronym,       % e.g.: UNGAOR
  addendum,
  casename,      % Smith v Jones
  chapter,       % e.g.: c C-12; c 1
  looseleaf,
  neutral,       % neutral citation
  offrep,        % official print reporter: SCR, FCR, Ex Cr
